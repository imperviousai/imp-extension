{
  "manifest_version": 2,
  "name": "Impervious",
  "short_name": "impervious-extension",
  "version": "0.1.0",
  "description": "The Impervious Browser Extension",
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "Impervious"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{fb005b54-5431-46cc-a3b2-7b6291bbf7cd}",
      "update_url": "https://raw.githubusercontent.com/imperviousai/imp-extension/master/updates.json"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  }
}
